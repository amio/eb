{
  "name": "ErrorBoard",
  "version": "2.0.0",
  "description": "Track and fix JavaScript errors fired by your visitor's browsers",
  "main": "eb.js",
  "config": {
    "dbfile": "db",
    "port": 3000
  },
  "scripts": {
    "start": "npm run browserify && node eb.js",
    "browserify": "browserify client/app.js -o client/public/app.js",
    "watchify": "watchify client/app.js -o client/public/app.js -v",
    "nodemon": "nodemon --ignore client/ eb.js"
  },
  "dependencies": {
    "nedb": "^0.10.5",
    "express": "^4.1.1",
    "moment": "^2.6.0",
    "envify": "^1.2.1",
    "lodash": "^2.4.1",
    "sockjs": "^0.3.8",
    "speakingurl": "^0.9.1",
    "useragent": "^2.0.8",
    "serve-favicon": "^2.0.0",
    "superagent": "^0.18.0",
    "vow": "^0.4.3"
  },
  "devDependencies": {
    "nodemon": "^1.0.17",
    "browserify": "^3.46.0",
    "browserify-shim": "^3.4.1",
    "watchify": "^0.8.3",
    "reactify": "^0.13.1",
    "page": "^1.3.7",
    "react": "^0.10.0"
  },
  "browser": {
    "react": "./node_modules/react/addons.js"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
